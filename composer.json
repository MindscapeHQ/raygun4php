{
    "name": "mindscape/raygun4php",
    "type": "library",
    "description": "Raygun.io client for sending errors and exceptions automatically.",
    "keywords": [
        "raygun",
        "errors",
        "exceptions",
        "logging"
    ],
    "homepage": "http://raygun.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Mindscape",
            "email": "contact@mindscape.co.nz",
            "homepage": "http://www.mindscapehq.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "guzzlehttp/guzzle": "~6.0",
        "psr/log": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "squizlabs/php_codesniffer": "^3.0",
        "estahn/phpunit-json-assertions": "^3.0"
    },
    "suggest": {
        "ext-curl": "*"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Raygun4php\\": "src/Raygun4php/",
            "Raygun4php\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    }
}
